{
  "name": "runstring",
  "version": "2.0.1",
  "description": "Convert JS functions to runnable strings. With parameter serialization!",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "lint": "eslint --ignore-path .gitignore . && tsc --noEmit",
    "lint-fix": "eslint --fix --ignore-path .gitignore . && tsc --noEmit",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.test.*\"",
    "coverage": "nyc --reporter=lcov npm test",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/runstring.git"
  },
  "keywords": [
    "functions",
    "string",
    "stringify",
    "runnable",
    "eval",
    "executable",
    "invoke"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/runstring/issues"
  },
  "homepage": "https://github.com/meyfa/runstring",
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@types/mocha": "9.0.0",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.1",
    "mocha": "9.1.3",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.5"
  }
}
